{
    "compilerOptions": {
        "strict": true,
        "target": "es5",
        "module": "ESNext",
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "outDir": "dist",
        "skipLibCheck": true,
        "declaration": true,
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext",
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "src/testApp/**/*.*"
    ]
}